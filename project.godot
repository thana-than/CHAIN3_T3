; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[CHAIN3_T3]

config/ninetailsrabbit.interaction_kit_3d/update_notification_enabled=true
config/ninetailsrabbit.interaction_kit_3d/interactables_collision_layer=6
config/ninetailsrabbit.interaction_kit_3d/grabbables_collision_layer=7

[Fmod]

General/auto_initialize=true
General/channel_count=1024
General/is_live_update_enabled=true
General/is_memory_tracking_enabled=false
"Software Format/sample_rate"=48000
"Software Format/speaker_mode"=3
"Software Format/raw_speaker_count"=0
General/default_listener_count=1
General/banks_path="res://fmod/Build/Desktop"
Plugins/path_to_plugin_configuration=""
General/should_load_by_name=false
DSP/dsp_buffer_size=512
DSP/dsp_buffer_count=4
"3D Settings/doppler_scale"=1.0
"3D Settings/distance_factor"=1.0
"3D Settings/rolloff_scale"=1.0

[application]

config/name="CHAIN3_T3"
run/main_scene="uid://cv5ho4r8agiy0"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"
boot_splash/new_file_template="~ start
Nathan: [[Hi|Hello|Howdy]], this is some dialogue.
Nathan: Here are some choices.
- First one
	Nathan: You picked the first one.
- Second one
	Nathan: You picked the second one.
- Start again => start
- End the conversation => END
Nathan: For more information see the online documentation.
=> END"

[autoload]

Config="*res://scripts/autoload/config_reader.gd"
FmodManager="*res://addons/fmod/FmodManager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
GlobalInteraction3D="*res://addons/ninetailsrabbit.interaction_kit_3d/src/autoload/global_interaction.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
Settings="*res://scripts/autoload/settings.gd"
QuitProcess="*res://scripts/autoload/quit_process.gd"
Chain3Adapter="*res://scripts/autoload/chain3_adapter.gd"
chain3="*res://addons/chain3/autoload.gd"

[debug_draw_3d]

settings/addon_root_folder="res://addons/debug_draw_3d"

[dialogue_manager]

runtime/advanced/uses_dotnet=true
editor/new_file_template="~ start

=> END"
runtime/balloon_path="res://assets/dialogue/styles/gothic_silent_hill/Balloon.tscn"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[display]

window/size/mode=4

[dotnet]

project/assembly_name="CHAIN3_T3"

[editor_plugins]

enabled=PackedStringArray("res://addons/chain3/plugin.cfg", "res://addons/character-controller/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/fmod/plugin.cfg", "res://addons/ninetailsrabbit.interaction_kit_3d/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/portals/plugin.cfg", "res://addons/room_gizmos/plugin.cfg")

